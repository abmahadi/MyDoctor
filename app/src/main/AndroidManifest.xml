<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mydoctor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RecyclerView.InDateAppoinmentActivity"
            android:label="Appoinments"></activity>
        <activity android:name=".Activity.AppoinmentDetailsActivity" />
        <activity android:name=".RecyclerView.AppoinmentListActivity"
            android:label="Appoinments"/>
        <activity android:name=".Activity.DoctorDetailsActivity"
            android:label="Doctors Details"/>
        <activity android:name=".Activity.AppoinmentActivity" />
        <activity android:name=".RecyclerView.DoctorListRecyclerViewActivity"
            android:label="Doctors List"/>
        <activity android:name=".Activity.DoctorsInformationActivity" />
        <activity android:name=".Activity.DoctorsMenuActivity" />
        <activity android:name=".Activity.DoctorSignInActivity" />
        <activity android:name=".Activity.DoctorsLoginActivity"
            android:label="Doctor LogIn"/>
        <activity android:name=".Activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.DoctorFindActivity" />
        <activity android:name=".Activity.PatientSignupActivity" />
        <activity android:name=".Activity.PatientLoginActivity"
            android:label="Patient LogIn"/>
    </application>

</manifest>