        <activity android:name=".Activity.DoctorsInformationActivity" />
        <activity android:name=".Activity.DoctorsMenuActivity" />
        <activity android:name=".Activity.DoctorSignInActivity" />
        <activity android:name=".Activity.DoctorsLoginActivity" />
        <activity android:name=".Activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.DoctorFindActivity" />
        <activity android:name=".Activity.PatientSignupActivity" />
        <activity android:name=".Activity.PatientLoginActivity" />
